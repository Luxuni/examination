{
  "name": "examination",
  "displayName": "examination",
  "version": "0.0.1",
  "description": " Code review plugin",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "build": "webpack -c webpack.prod.js",
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "dev": "webpack-dev-server -c webpack.dev.js",
    "lint": "eslint src --ext ts",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "pnpm run build && pnpm run package",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "contributes": {
    "colors": [
      {
        "id": "examination.codeBackground",
        "description": "Background decoration color for selected code",
        "defaults": {
          "dark": "#fc570b55",
          "light": "#fc570b55",
          "highContrast": "#fc0b0b55"
        }
      }
    ],
    "commands": [
      {
        "command": "examination.selectText",
        "title": "Select Text"
      },
      {
        "command": "examination.myCommand",
        "title": "User Command"
      }
    ],
    "configuration": {
      "title": "Examination Settings",
      "properties": {
        "examination.message": {
          "type": "object",
          "default": {},
          "description": "examination message."
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "examination.selectText",
          "group": "navigation"
        }
      ]
    }
  },
  "activationEvents": [],
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.5",
    "@reduxjs/toolkit": "^2.2.1",
    "ahooks": "^3.7.10",
    "antd": "^5.14.2",
    "darkreader": "^4.9.79",
    "form-render": "^2.4.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-syntax-highlighter": "^15.5.0",
    "react-transition-group": "^4.4.5",
    "simple-git": "^3.24.0",
    "table-render": "^2.1.4",
    "umi-request": "^1.4.0",
    "uuid": "^9.0.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/react-transition-group": "^4.4.10",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@umijs/lint": "^4.1.10",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.12",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "vscode": "^1.86.0"
  }
}
